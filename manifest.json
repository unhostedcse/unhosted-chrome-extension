{
  "name": "Unhosted Mail",
  "description": "Chrome App for TCP connection",
  "version": "1.1.2",
	
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
    
    },
    "externally_connectable": {      
	"matches": ["http://localhost/*","http://unhostedcse.github.io/*","http://unhosted.projects.uom.lk/*"]
    },
   "sockets":{
      "tcp":{
        "connect": ""
      }
   },
   "permissions": [
      "http://127.0.0.1/*"
   ],
  "icons": { "16": "img/icon_16.png", "128": "img/icon_128.png" }

}
